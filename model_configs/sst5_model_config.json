{
    "env": {
        "USE_GPU": 0
    },

    "dataset": {
        "select_dataset": "sentiment",
        "sst": {
            "granularity": "2-class",
            "transformer_model_name": "roberta-base",
            "use_subtrees": false,
            "proportion": 1,
            "max_length": 500,
            "noise_datapath": "none"
        },
        "sentiment": {
            "datapath": "data/sst/sst_5/SST",
            "noisy_datapath": "all",
            "transformer_model_name": "roberta-base",
            "max_length": 500
        }
    },

    "text_model": {
        "embedder": {
            "model_name": "roberta-base",
            "train_parameters": true
        },
        "encoder": {
            "model_name": "roberta-base",
            "dropout": 0.3
        },
        "classifier": {
            "feedforward": {
                "num_layers": 3,
                "hidden_dims": [300, 150, 75],
                "activations": "silu",
                "dropout": 0.3
            }
        },
        "optimizer": {
            "select_optimizer": "adamw",
            "lr": 1e-5
        },
        "scheduler": {
            "select_scheduler": "reduce",
            "training_steps": 1600,
            "warmup_steps": 96
        },
        "criterions": {
            "classification_criterion": "cross_entropy",
            "contrastive_criterion": "consine_embedding"
        },
        "evaluation": "categorical",
        "clip_grad": {
            "is_clip": true,
            "max_norm": 0.9,
            "norm_type": 2
        }
    },

    "reinforcer": {
        "environment": {
            "similarity_threshold": 0.8,
            "maximize_target": "ib"
        },

        "augmenter": {
            "synonyms": {
                "synonyms_filepath": null
            },
            "select_augmenter": [0, 1, 2, 3],
            "delete_augmenter": {
                "magnitude": 0.1
            },
            "swap_augmenter": {
                "magnitude": 0.1
            },
            "replace_augmenter": {
                "magnitude": 0.1
            },
            "insert_augmenter": {
                "magnitude": 0.1
            },
            "backtrans_augmenter": {
                "from_model_name": "Helsinki-NLP/opus-mt-en-de",
                "to_model_name": "Helsinki-NLP/opus-mt-de-en",
                "device": "cuda",
                "magnitude": 1
            }
        },

        "policy": {
            "feedforward": {
                "num_layers": 3,
                "hidden_dims": [384, 96],
                "activations": "relu",
                "dropout": 0.3
            },

            "optimizer": {
                "select_optimizer": "adam",
                "lr": 1e-4
            }
        },

        "REINFORCE": {
            "gamma": 0.90,
            "clip_grad": 3,
            "max_step": 6
        }
    },

    "visualizer": {
        "mode": "isomap",
        "tsne": {
            "col_names": [
                "tsne_1",
                "tsne_2",
                "category"
            ],
            "n_components": 2,
            "perplexity": [
                5, 10, 15, 20, 30,
                40, 50, 60, 70, 80,
                90, 100, 110, 120, 130,
                140, 150, 200, 300, 500,
                750, 1000
            ],
            "n_iter": 1000,
            "verbose": 1,
            "save_fig_path": "model_record/visual/"
        },
        "isomap": {
            "col_names": [
                "isomap_1",
                "isomap_2",
                "category"
            ],
            "n_neighbors": 5,
            "n_components": 2,
            "n_jobs": 6,
            "save_fig_path": "model_record/visual/isomap/"
        }
    },

    "train_mode": {
        "select_mode": 0,
        "0": {
            "text_trainer": {
                "dataloader": {
                    "train_batch_size": 32,
                    "valid_batch_size": 200,
                    "test_batch_size": 200,
                    "shuffle": true
                },
                "accumulated_step": 1,
                "epochs": 6,
                "is_save": true
            },
            "reinforce_trainer": {
                "dataloader": {
                    "shuffle": true
                },
                "epochs": 1,
                "is_save": true,
                "is_writer": true,
                "batch_size": 120
            },
            "augmented_instance_generator": {
                "num_processor": 5,
                "select_mode": "default",
                "select_policy": [
                    "model_record/reinforce_model_weights/policy30",
                    "model_record/reinforce_model_weights/policy40",
                    "model_record/reinforce_model_weights/policy50",
                    "model_record/reinforce_model_weights/policy60",
                    "model_record/reinforce_model_weights/policy70"
                ],
                "save_name": [
                    "model_record/augments/augment30",
                    "model_record/augments/augment40",
                    "model_record/augments/augment50",
                    "model_record/augments/augment60",
                    "model_record/augments/augment70"
                ]
            },
            "text_finetuner": {
                "dataloader": {
                    "train_batch_size": 16,
